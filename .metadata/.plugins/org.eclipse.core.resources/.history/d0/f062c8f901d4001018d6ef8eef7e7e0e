	private javax.sql.DataSource ds;
	
    public ImageBean() {
        // TODO Auto-generated constructor stub
    }
    
    public int UploadImg(){
    	conn= ds.getConnection();
    	 blobFileUse(db.con);
    	
    }
    

}
